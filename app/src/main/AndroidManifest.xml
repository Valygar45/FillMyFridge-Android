<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.utt.if26.fillmyfridge">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fridgeicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fridgeicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GenerateListMenuDatesActivity"
            android:windowSoftInputMode="stateAlwaysHidden|adjustPan"
            />
        <activity android:name=".GenerateListMenuMealNumberActivity" />
        <activity android:name=".GenerateListMenuPersonPerMealActivity" />
        <activity android:name=".GenerateListMenuMealThemeActivity" />
        <activity android:name=".GenerateListMenuEnding" />
        <activity android:name=".ListeDeCourseActivity" />
        <activity android:name=".ListMenuDetailsActivity" />
        <activity android:name=".AllListMenuActivity" />
        <activity android:name=".RepasDetails" />
        <activity android:name=".AProposActivity"></activity>
    </application>

</manifest>